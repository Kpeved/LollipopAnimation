<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:top="1dp" android:left="1dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/shadow"/>
        </shape>
    </item>
    <item  android:right="1dp" android:bottom="1dp" android:left="1dp" android:top="1dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/field_green"/>
        </shape>
    </item>
    <item android:left="15dp" android:right="15dp" android:bottom="15dp" android:top="15dp">
        <bitmap
            android:src="@drawable/ic_action_plus">
        </bitmap>
    </item>


</layer-list>
